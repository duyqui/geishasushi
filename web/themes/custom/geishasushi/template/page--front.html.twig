      <span class="glf-button-default"
            data-glf-cuid="" data-glf-ruid="9dbe72d4-54ae-46a7-890c-2ff6126ab915" id="glfButton0">
        <img class="img-button-online-bestellung-red" src="{{ active_theme_path() }}/images/Button_Online_red.png">
      </span>
      <script src="https://www.fbgcdn.com/embedder/js/ewm2.js" defer="" async=""></script>
    </button>
    <img class="img-special" src="{{ active_theme_path() }}/images/Special.png">
  </div>
  <div class="lokal">
