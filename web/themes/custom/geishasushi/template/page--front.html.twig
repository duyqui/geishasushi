      <div class="menu">MENU</div>
      <img class="img-logo" src="{{ active_theme_path() }}/images/Logo.png">
      <div class="menu">ONLINEBESTELLUNG</div>
      <div class="menu">KONTAKT</div>
    </div>
{#      <div class="cart">#}
{#        <img class="img-cart" src="{{ active_theme_path() }}/images/cart.png">#}
{#      </div>#}
    <div class="txt-online-bestellung">
